tions, mapState } from 'vuex';\n\nexport default {\n  name: 'Header',\n  computed: {\n    ...mapState(['userLoggedIn']),\n  },\n  methods: {\n    ...mapMutations(['toggleAuthModal']),\n    signout() {\n      this.$store.dispatch('signout', {\n        router: this.$router,\n        route: this.$route,\n      });\n\n      if (this.$route.meta.requiresAuth) {\n        this.$router.push({ name: 'home' });\n      }\n    },\n\n  },\n};\n</script>\n"]}]}