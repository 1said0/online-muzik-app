   this.songs[i].modified_name = values.modified_name;\n      this.songs[i].genre = values.genre;\n    },\n    removeSong(i) {\n      this.songs.splice(i, 1);\n    },\n    addSong(document) {\n      const song = {\n        ...document.data(),\n        docID: document.id,\n      };\n\n      this.songs.push(song);\n    },\n    updateUnsavedFlag(value) {\n      this.unsavedFlag = value;\n    },\n  },\n  beforeRouteLeave(to, from, next) {\n    if (!this.unsavedFlag) {\n      next();\n    } else {\n      const leave = confirm('Kaydedilmemiş değişiklikleriniz var. Gitmek istediğinizden emin misiniz?');\n      next(leave);\n    }\n  },\n\n};\n</script>\n"]}]}