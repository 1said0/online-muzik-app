;AAAA;AAAA,qBACX,MAAI,CAAC,cADM;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAKf,gBAAA,MAAI,CAAC,cAAL,GAAsB,IAAtB;;AALe,qBASX,MAAI,CAAC,KAAL,CAAW,MATA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAUS,eAAc,CACjC,GADmB,CACf,MAAI,CAAC,KAAL,CAAW,MAAI,CAAC,KAAL,CAAW,MAAX,GAAoB,CAA/B,EAAkC,KADnB,EAEnB,GAFmB,EAVT;;AAAA;AAUP,gBAAA,OAVO;AAAA;AAAA,uBAaK,eAAc,CAC7B,OADe,CACP,eADO,EAEf,UAFe,CAEJ,OAFI,EAGf,KAHe,CAGT,MAAI,CAAC,UAHI,EAIf,GAJe,EAbL;;AAAA;AAab,gBAAA,SAba;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAmBK,eAAc,CAC7B,OADe,CACP,eADO,EAEf,KAFe,CAET,MAAI,CAAC,UAFI,EAGf,GAHe,EAnBL;;AAAA;AAmBb,gBAAA,SAnBa;;AAAA;AAyBf,gBAAA,SAAS,CAAC,OAAV,CAAkB,UAAC,QAAD,EAAc;AAC9B,kBAAA,MAAI,CAAC,KAAL,CAAW,IAAX;AACE,oBAAA,KAAK,EAAE,QAAQ,CAAC;AADlB,qBAEK,QAAQ,CAAC,IAAT,EAFL;AAID,iBALD;AAOA,gBAAA,MAAI,CAAC,cAAL,GAAsB,KAAtB;;AAhCe;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiChB;AA3CM;AApBI,CAAf","sourcesContent":["<template>\n  <main>\n    <!-- Introduction -->\n    <section class=\"mb-8 py-20 text-white text-center relative\">\n      <div class=\"absolute inset-0 w-full h-full bg-contain introduction-bg\"\n        style=\"background-image: url(assets/img/header.png)\"></div>\n      <div class=\"container mx-auto\">\n        <div class=\"text-white main-header-content\">\n          <h1 class=\"font-bold text-5xl mb-5\">Müzik dinle!</h1>\n          <p class=\"w-full md:w-8/12 mx-auto\">\n            Müzik yükle ve dinlemeye başla\n          </p>\n        </div>\n      </div>\n\n      <img class=\"relative block mx-auto mt-5 -mb-20 w-auto max-w-full\" src=\"assets/img/introduction-music.png\" />\n    </section>\n\n    <!-- Main Content -->\n    <section class=\"container mx-auto\">\n      <div class=\"bg-white rounded border border-gray-200 relative flex flex-col\">\n        <div class=\"px-6 pt-6 pb-5 font-bold border-b border-gray-200\">\n          <span class=\"card-title\">Müzikler</span>\n          <!-- Icon -->\n          <i class=\"fa fa-headphones-alt float-right text-green-400 text-2xl\"></i>\n        </div>\n        <!-- Playlist -->\n        <ol id=\"playlist\">\n          <app-song-item v-for=\"song in songs\" :key=\"song.docID\" :song=\"song\" />\n        </ol>\n        <!-- .. end Playlist -->\n      </div>\n    </section>\n  </main>\n</template>\n\n<script>\nimport { songsCollection } from '@/includes/firebase';\nimport AppSongItem from '@/components/SongItem.vue';\n\nexport default {\n  name: 'Home',\n  components: {\n    AppSongItem,\n  },\n  data() {\n    return {\n      songs: [],\n      maxPerPage: 25,\n      pendingRequest: false,\n    };\n  },\n  async created() {\n    this.getSongs();\n\n    window.addEventListener('scroll', this.handleScroll);\n  },\n  beforeUnmount() {\n    window.removeEventListener('scroll', this.handleScroll);\n  },\n  methods: {\n    handleScroll() {\n      const { scrollTop, offsetHeight } = document.documentElement;\n      const { innerHeight } = window;\n      const bottomOfWindow = Math.round(scrollTop) + innerHeight === offsetHeight;\n\n      if (bottomOfWindow) {\n        this.getSongs();\n      }\n    },\n    async getSongs() {\n      if (this.pendingRequest) {\n        return;\n      }\n\n      this.pendingRequest = true;\n\n      let snapshots;\n\n      if (this.songs.length) {\n        const lastDoc = await songsCollection\n          .doc(this.songs[this.songs.length - 1].docID)\n          .get();\n        snapshots = await songsCollection\n          .orderBy('modified_name')\n          .startAfter(lastDoc)\n          .limit(this.maxPerPage)\n          .get();\n      } else {\n        snapshots = await songsCollection\n          .orderBy('modified_name')\n          .limit(this.maxPerPage)\n          .get();\n      }\n\n      snapshots.forEach((document) => {\n        this.songs.push({\n          docID: document.id,\n          ...document.data(),\n        });\n      });\n\n      this.pendingRequest = false;\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}